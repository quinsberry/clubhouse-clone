{
  "name": "clubhouse-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "client:dev": "next dev",
    "client:build": "next build",
    "server:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register server/app.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "test": "jest --logHeapUsage"
  },
  "jest": {
    "moduleNameMapper": {
      "^@api(.*)$": "<rootDir>/api$1",
      "^@components(.*)$": "<rootDir>/components$1",
      "^@core(.*)$": "<rootDir>/core$1",
      "^@generated(.*)$": "<rootDir>/generated$1",
      "^@hooks(.*)$": "<rootDir>/hooks$1",
      "^@pages(.*)$": "<rootDir>/pages$1",
      "^@services(.*)$": "<rootDir>/services$1",
      "^@store(.*)$": "<rootDir>/store$1",
      "^@styles(.*)$": "<rootDir>/styles$1",
      "^@tps(.*)$": "<rootDir>/tps$1",
      "^@utils(.*)$": "<rootDir>/utils$1",
      "^@server(.*)$": "<rootDir>/server$1"
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "cookie": "^0.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nanoid": "^3.1.23",
    "next": "10.0.9",
    "next-redux-wrapper": "^7.0.0",
    "node-sass": "^5.0.0",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "react": "17.0.2",
    "react-code-input": "^3.10.1",
    "react-dom": "17.0.2",
    "react-number-format": "^4.5.1",
    "react-redux": "^7.2.4",
    "sequelize": "^6.6.2",
    "sharp": "^0.28.2",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@types/cookie": "^0.4.0",
    "@types/cors": "^2.8.10",
    "@types/express": "4.17.12",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.36",
    "@types/passport": "^1.0.6",
    "@types/passport-github": "^1.1.5",
    "@types/passport-jwt": "^3.0.5",
    "@types/react": "^17.0.3",
    "@types/sharp": "^0.28.1",
    "babel-jest": "^27.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.4",
    "jest-dom": "^4.0.0",
    "sequelize-cli": "^6.2.0",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
